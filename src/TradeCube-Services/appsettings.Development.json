{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "RedisConfiguration": {
    "AbortOnConnectFail": "false",
    "KeyPrefix": "TradeCubeServices",
    "Hosts": [
      {
        "Host": "redis",
        "Port": "6379"
      }
    ],
    "AllowAdmin": "true",
    "ConnectTimeout": "3000",
    "Database": "0",
    "ServerEnumerationStrategy": {
      "Mode": "All",
      "UnreachableServerAction": "IgnoreIfOtherAvailable",
      "TargetRole": "Any"
    }
  }
}